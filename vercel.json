{
	"version": 2,
	"builds": [{ "src": "./index.js", "use": "@vercel/node" }],
    "build": {
        "env": {
          "NODE_ENV": "production"
        },
        "commands": [
          "npm install",
          "node db/initDB.js",
          "npm run build"
        ]
      },
	"rewrites": [{ "source": "/(.*)", "destination": "./index.js" }]
}